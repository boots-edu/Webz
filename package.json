{
    "name": "@gsilber/webez",
    "version": "0.1.2",
    "description": "",
    "main": "dist/index.js",
    "scripts": {
        "build": "rm -rf ./dist && tsc && rm -rf ./dist/tests && cp ./package.json dist",
        "test": "jest",
        "watch": "jest --watchAll=true",
        "coverage": "jest --coverage --watchAll=false",
        "test:json": "jest --json --watchAll=false --outputFile jest-output.json --coverage",
        "lint": "eslint . --ext .ts",
        "doc": "typedoc --entryPoints src",
        "lint:fix": "eslint . --ext .ts --fix",
        "format": "prettier \"(src|test)/**/*\" --ignore-unknown",
        "pub": "cp ./package.json dist && cd ./dist && npm publish --access public"
    },
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@jest/globals": "^29.7.0",
        "@types/jest": "^29.5.12",
        "@typescript-eslint/eslint-plugin": "^6.2.0",
        "@typescript-eslint/parser": "^6.2.0",
        "eslint": "^8.46.0",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "prettier": "^3.2.4",
        "reflect-metadata": "^0.2.1",
        "ts-jest": "^29.1.2",
        "ts-node": "^10.9.2",
        "typescript": "^5.3.3"
    }
}
