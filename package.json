{
  "name": "@gsilber/webez",
  "version": "1.1.2",
  "description": "",
  "main": "dest/index.js",
  "scripts": {
    "start": "tsc && node dest/test.js",
    "build": "tsc",
    "test": "jest",
    "watch": "jest --watchAll=true",
    "coverage": "jest --coverage --watchAll=false",
    "test:json": "jest --json --watchAll=false --outputFile jest-output.json --coverage",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier \"(src|test)/**/*\" --ignore-unknown",
    "publish": "npm publish --access public"

  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "resetMocks": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/",
      "!src/serviceWorker.js",
      "!src/setupTests.js",
      "!src/components/Auth/*",
      "!src/index.js"
    ],
    "coverageReporters": [
      "text",
      "lcov"
    ]
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "eslint": "^8.46.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
